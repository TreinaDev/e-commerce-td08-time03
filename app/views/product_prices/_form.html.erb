<div>
  <%= form_with(model: [@product_model, @price]) do | f | %>
    <div>
      <%= f.label :price %>
      <%= f.number_field :price %>
    </div>
    <div>
      <%= f.label :start_date %>
      <%= f.text_field :start_date %>
    </div>
     <div>
      <%= f.label :end_date %>
      <%= f.text_field :end_date %>
    </div>
    <%= f.submit %>
  <% end %>
</div>